<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="SoftwareSelectionList" type="选课列表类型"/>
   
    <xs:complexType name ="选课列表类型">
    	<xs:sequence minOccurs="0" maxOccurs="unbounded">
   			<xs:element name="SoftwareSelection" type="选课信息类型"/>
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="选课信息类型">
    	<xs:sequence>
    		<xs:element name="课程编号" type="课程编号类型"/>
    		<xs:element name="学生编号" type="学生编号类型"/>
    		<xs:element name="成绩" type="成绩类型"/>
    	</xs:sequence>
    </xs:complexType>

    <xs:simpleType name="课程编号类型">
        <xs:restriction base="xs:string">
            <xs:length value="8"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="学生编号类型">
    	<xs:restriction base="xs:string">
            <xs:length value="9"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="成绩类型">
        <xs:restriction base="xs:string">
            <xs:length value="4"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>